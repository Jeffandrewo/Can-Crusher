{
    "version": 2,
    "functions": {
      "api/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/api/check-bins",
        "dest": "/api/index.js"
      }
    ],
    "crons": [
      {
        "path": "/api/check-bins",
        "schedule": "*/10 * * * *"
      }
    ]
  }